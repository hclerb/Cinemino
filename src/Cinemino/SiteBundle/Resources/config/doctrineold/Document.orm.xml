<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cinemino\SiteBundle\Entity\Document" table="document">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomDocument" type="string" column="NOM_DOCUMENT" length="25" nullable="true"/>
    <field name="descriptifDocument" type="string" column="DESCRIPTIF_DOCUMENT" length="100" nullable="true"/>
    <field name="urlFichier" type="string" column="URL_FICHIER" length="50" nullable="true"/>
  </entity>
</doctrine-mapping>
