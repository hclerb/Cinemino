
{% extends "CineminoSiteBundle::interlayout.html.twig" %}
{% block title %}
{{ parent() }} - Intervenants
{% endblock %}

{% block formulaire %}


<div id="formulaire" class="well" style="width:800px">
<div class="page-header"><h3>Ajouter un intervenant</h3></div>   
<form action="{{ path('intervenant_create') }}" method="post" {{ form_enctype(form) }}>
    {{ tinymce_init() }}      
    {{ form_errors(form) }}
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('intervenant') }}">
            Back to the list
        </a>
    </li>
</ul>

</div>

{% endblock %}